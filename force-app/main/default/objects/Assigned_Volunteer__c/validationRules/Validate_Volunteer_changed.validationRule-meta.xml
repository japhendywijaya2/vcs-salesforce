<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Volunteer_changed</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT(ISNEW()) ,  Contact__c != PRIORVALUE(Contact__c ))</errorConditionFormula>
