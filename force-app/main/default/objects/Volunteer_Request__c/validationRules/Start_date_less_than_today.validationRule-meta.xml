<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_date_less_than_today</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(), Start_Date__c &lt;  TODAY() )</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date must be greater than today.</errorMessage>
</ValidationRule>
